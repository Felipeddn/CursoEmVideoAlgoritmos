algoritmo "conversor de angulos para seno"
// Função : converte valores
// Autor : Gustavo Guanabara
// Data : 02/03/2022
var
   angulo, seno: real
inicio
      Escreva("Informe um angulo: ")
      Leia(angulo)
      seno <- Sen(GraupRad(angulo))
      Escreva("O seno do angulo ", angulo, " é igual a ", seno)
fimalgoritmo
