algoritmo "Conte Até 10"
// Função : Conta até 10
// Autor : Gustavo Guanabara
// Data : 24/03/2022
var
   contador : inteiro
inicio
      contador <- 0
      Enquanto (contador <= 10) faca
               Escreval(contador)
               contador <- contador + 1
      FimEnquanto
      Escreva("Terminei de contar.")
fimalgoritmo
