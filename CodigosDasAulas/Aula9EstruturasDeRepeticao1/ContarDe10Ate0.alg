algoritmo "Contar de 10 até 0"
// Função : conta de 10 até 0
// Autor : Gustavo Guanabara
// Data : 25/03/2022
var
   contador : inteiro
inicio
      Contador <- 10
      enquanto (contador >= 0) faca
               Escreval(contador)
               contador <- contador - 1
      fimenquanto
      escreva("terminei de contar")
fimalgoritmo
